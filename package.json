{
  "name": "serverless-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Serverless Typescript Boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/SamNewhouse/serverless-typescript-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "offline": "serverless offline",
    "format": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,jsx,ts,tsx,json,yml,yaml,md}\" --write",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "tail:hello:dev": "serverless logs --function hello --tail --stage dev",
    "deploy": "serverless deploy"
  },
  "author": "Sam Newhouse",
  "dependencies": {
    "dotenv": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.93",
    "@types/jest": "^27.0.2",
    "@types/node": "17.0.23",
    "aws-lambda": "1.0.7",
    "dotenv-webpack": "^7.0.3",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "serverless": "3.12.0",
    "serverless-iam-roles-per-function": "3.2.0",
    "serverless-offline": "8.5.0",
    "serverless-prune-plugin": "2.0.1",
    "serverless-webpack": "5.6.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.71.0",
    "webpack-node-externals": "3.0.0"
  }
